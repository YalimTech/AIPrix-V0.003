# Configuración npm ULTRA-AGRESIVA para eliminar TODOS los warnings
production=false
optional=true
legacy-peer-deps=true
fund=false
audit=false
save-exact=false
package-lock=true
engine-strict=false
# Eliminar TODOS los warnings
warn=false
warn-level=error
loglevel=error
silent=true
# Configuración específica para eliminar warnings de npm
npm_config_warn=false
npm_config_loglevel=error
npm_config_silent=true
# Eliminar warnings de workspace
workspace-config=false