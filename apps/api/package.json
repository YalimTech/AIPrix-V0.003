{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "npx prisma generate",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:api": "cross-env-shell \"npx dotenv -e ../../.env -- nest start --watch\"",
    "start:dev": "npx dotenv -e ../../.env -- nest start --watch",
    "start:debug": "cross-env-shell \"npx dotenv -e ../../.env -- nest start --debug --watch\"",
    "start:prod": "npm run db:setup && node dist/main.js",
    "start:prod:direct": "node dist/main.js",
    "start:prod:api": "node start-production.js",
    "db:setup": "npx prisma generate && (npx prisma migrate deploy || npx prisma db push --accept-data-loss || echo 'DB setup failed, continuing...')",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:unit": "jest --testPathPattern=spec.ts",
    "test:integration": "jest --testPathPattern=e2e-spec.ts",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "db:seed": "npx ts-node prisma/seed.ts",
    "db:reset": "npx prisma migrate reset --force && npm run db:seed",
    "backfill:countries": "ts-node -r tsconfig-paths/register src/scripts/backfill-countries.ts",
    "setup:credentials": "node scripts/setup-credentials.js",
    "verify:credentials": "node scripts/verify-credentials.js",
    "setup:complete": "npm run setup:credentials && npm run db:setup && npm run verify:credentials"
  },
  "dependencies": {
    "@elevenlabs/elevenlabs-js": "^2.19.0",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "4.0.0",
    "@nestjs/core": "11.0.0",
    "@nestjs/event-emitter": "3.0.0",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "11.0.0",
    "@nestjs/platform-socket.io": "11.0.0",
    "@nestjs/platform-ws": "^10.4.20",
    "@nestjs/swagger": "7.4.2",
    "@nestjs/throttler": "6.0.0",
    "@nestjs/websockets": "11.0.0",
    "@prisma/client": "^6.18.0",
    "@types/crypto-js": "^4.2.2",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^7.0.1",
    "@types/pg": "^8.15.5",
    "@typescript-eslint/tsconfig-utils": "^8.44.1",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "csv-parser": "^3.0.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.6.1",
    "exceljs": "4.4.0",
    "express": "^4.18.2",
    "nodemailer": "^7.0.6",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "pgvector": "^0.2.1",
    "reflect-metadata": "^0.2.0",
    "set-cookie-parser": "2.7.1",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.10.1",
    "vite": "^7.1.10",
    "winston": "^3.11.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.0",
    "@types/twilio": "^3.19.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@typescript-eslint/project-service": "^8.44.1",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^6.18.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
